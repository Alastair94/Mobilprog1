<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hu.bead.dinosaurs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dinosaurs">
        <activity android:name=".OptionsActivity"></activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/lbl_settings"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FormActivity"
            android:label="@string/lbl_register" />
        <activity
            android:name=".DinoPageActivity"
            android:label="@string/lbl_data"
            android:parentActivityName=".DinoList" />
        <activity
            android:name=".DinoList"
            android:label="@string/lbl_list" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>